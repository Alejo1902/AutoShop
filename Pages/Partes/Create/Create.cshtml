@page
@model AutoShop.Pages.Partes.Create.CreateModel
@{
	ViewData["Title"] = "Nuevas Partes";
}
<h1>Nuevo Parte</h1>
<h4> PARTES </h4>
<hr />
<div class="row">
	<div class="col-md-4">
		<form method="post">
			<div asp-validation-summary="ModelOnly" class="text-danger"></div>
			<div class="form-group">
				<label asp-for="partes.IdParte"></label>
				<input asp-for="partes.IdParte" class="form-control" />
				<span asp-validation-for="partes.IdParte" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="partes.Nombre"></label>
				<input asp-for="partes.Nombre" class="form-control" />
				<span asp-validation-for="partes.Nombre" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="partes.NumeroParte"></label>
				<input asp-for="partes.NumeroParte" class="form-control" />
				<span asp-validation-for="partes.NumeroParte" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="partes.Precio"></label>
				<input asp-for="partes.Precio" class="form-control" />
				<span asp-validation-for="partes.Precio" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="partes.Stock"></label>
				<input asp-for="partes.Stock" class="form-control" />
				<span asp-validation-for="partes.Stock" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="partes.Proveedor"></label>
				<input asp-for="partes.Proveedor" class="form-control" />
				<span asp-validation-for="partes.Proveedor" class="text-danger"></span>
			</div>
		</form>
	</div>
</div>
@section Scripts {
			await Html.RenderPartialAsync("_ValidationScriptsPartial")

}